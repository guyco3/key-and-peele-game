{
  "name": "key-and-peele-game-backend",
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=tests/unit --no-cache",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=tests/integration --runInBand"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "rate-limiter-flexible": "^9.0.1",
    "redis": "^5.10.0",
    "socket.io": "^4.7.4",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.2",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}